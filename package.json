{
  "name": "dennnis.se",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "webpack-dev-server",
    "lint": "eslint 'src/{**/,}*.{js,ts,tsx}'",
    "prettier": "prettier --check 'src/{**/,}*.{js,ts,tsx}'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "debug": false,
          "useBuiltIns": "usage",
          "corejs": 3,
          "targets": {
            "browsers": [
              "ie >= 11",
              "safari >= 12"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "eslintConfig": {
    "extends": [
      "@iteam/eslint-config-iteam-node"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "modules": true
      },
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "overrides": [
      {
        "files": [
          "**/*.tsx",
          "**/*.ts",
          "**/*.js"
        ],
        "rules": {
          "no-undef": "off",
          "no-unused-vars": "off",
          "react/prop-types": "off",
          "space-before-function-paren": "off",
          "require-atomic-updates": "off"
        }
      }
    ],
    "rules": {
      "no-unexpected-multiline": "error",
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "author": "Dennis Pettersson <dennis.pettersson@gmail.se>",
  "dependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@iteam/eslint-config-iteam-node": "0.1.0",
    "@iteam/eslint-config-node": "0.3.0",
    "@types/node": "13.13.5",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.5",
    "css-loader": "3.5.3",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "html-webpack-plugin": "4.5.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-entypo": "1.3.0",
    "react-inlinesvg": "1.2.0",
    "react-router-dom": "5.1.2",
    "react-three-fiber": "4.2.3",
    "style-loader": "1.2.1",
    "svg-inline-loader": "0.8.2",
    "tailwindcss": "1.4.6",
    "tailwindcss-animatecss": "0.3.3",
    "ts-loader": "7.0.3",
    "ts-node": "9.0.0",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "devDependencies": {
    "source-map": "0.7.3",
    "source-map-loader": "0.2.4",
    "source-map-support": "0.5.19",
    "ts-node-dev": "1.0.0-pre.44",
    "webpack-dev-server": "3.11.0"
  },
  "optionalDependencies": {
    "husky": "4.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier"
    }
  }
}
